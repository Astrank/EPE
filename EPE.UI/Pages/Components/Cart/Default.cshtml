@model IEnumerable<EPE.Application.Cart.GetCart.Response>

<div class="container">
    <h2>Big cart component</h2>
    @foreach (var product in Model)
    {
        <p>@product.Name</p>
        <p>@product.Value</p>
        <p>@product.Qty</p>
        <p>@product.StockId</p>
        <hr/>
    }
</div>